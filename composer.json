{
    "name": "clarkwinkelmann/flarum-500led",
    "description": "Simulate CSV animation files on a LED tree",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.0",
        "flarum/bbcode": "*",
        "flarum/tags": "*",
        "fof/upload": "*"
    },
    "authors": [
        {
            "name": "Clark Winkelmann",
            "email": "clark.winkelmann@gmail.com",
            "homepage": "https://clarkwinkelmann.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/clarkwinkelmann/flarum-500led/issues",
        "source": "https://github.com/clarkwinkelmann/flarum-500led"
    },
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\LED500\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "500 LED",
            "category": "feature",
            "icon": {
                "name": "fas fa-lightbulb",
                "backgroundColor": "#684ba6",
                "color": "#fff"
            }
        }
    }
}
